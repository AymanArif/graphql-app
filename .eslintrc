{
    "plugins": ["jest"],
    "rules": {
      "import/no-unresolved": "off",
      "react/prop-types": "off",
      "react/jsx-pascal-case": "off",
      "import/order": "off",
      "babel/camelcase": "off",
      "jsx-a11y/label-has-for": "off",
      "max-lines-per-function": "off"
    },
    "env": {
      "jest": true,
      "jest/globals": true
    }
  }